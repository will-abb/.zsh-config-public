########### Custom Aliases ###########
# 1. shell
alias clip="xclip -selection clipboard"
alias copy="copyfile"
alias z="source ~/.zshrc"
alias c='cls'
alias fox="/opt/firefox/firefox -P "work" --no-remote > /dev/null 2>&1 &"
alias py="python3"
alias p="pushd"
alias P="popd"
# 2.git
alias pre="pre-commit run --all-files"
alias m="git mv"
alias greset="git reset --hard HEAD~1"
# 3.terraform
alias ti="terraform init"
alias ta="terraform apply"
alias td="terraform destroy"
alias tp="terraform plan"
alias ts="terraform show"
alias tf="terraform fmt"
alias rt="rm -rf .terraform && rm .terraform.lock.hcl"
# 4.gpt
alias sx="sgpt --shell '"
alias sl="sgpt --chat linux '"
alias sa="sgpt --chat AWS '"
alias sg="sgpt --chat  git'"
# 5.traverse directories
alias pd="cd ~/repositories/bitbucket/williseed1"
alias wd="cd ~/repositories/bitbucket/SelectQuote"
alias down="cd ~/Downloads"
# 6.ansible
alias ap="ansible-playbook "
alias ai="ansible-inventory "
alias av="ansible-vault "
# 7.aws
alias sso="aws sso login --profile play"
alias sts="aws sts get-caller-identity"
alias ec2="aws ec2 describe-instances --instance-id"
alias ssm="aws ssm start-session --target"
alias unset-aws="unset AWS_ACCESS_KEY_ID; unset AWS_SECRET_ACCESS_KEY; unset AWS_REGION; unset AWS_PROFILE;"
alias dev="export AWS_PROFILE=dev"
alias play="export AWS_PROFILE=play"
alias prod="export AWS_PROFILE=prod"
alias uat="export AWS_PROFILE=uat"
alias devr="export AWS_PROFILE=dev-read"
alias playr="export AWS_PROFILE=play-read"
alias prodr="export AWS_PROFILE=prod-read"
alias uatr="export AWS_PROFILE=uat-read"
# 8.linux
alias update="sudo apt update && sudo apt upgrade -y && sudo snap refresh"
alias ke="killall emacs"
alias exp="nautilus . &"
alias chrysalis="/opt/Chrysalis-linux-x64/Chrysalis"
alias gex="gnome-extensions"
alias gshell="gnome-shell"
# 9.virtual_env
alias vanki="source ~/virtual_environments/anki/bin/activate"
alias vansible="source ~/virtual_environments/ansible/bin/activate"
alias vgpt="source ~/virtual_environments/gpt/bin/activate"
alias vscripts="source ~/virtual_environments/scripts/bin/activate"
alias vsecrets="source ~/virtual_environments/secrets/bin/activate"
alias vspeech="source ~/virtual_environments/speech/bin/activate"
alias vwhisper="source ~/virtual_environments/whisper/bin/activate"
alias vselenium="source ~/virtual_environments/selenium/bin/activate"
alias vhealthchecks="source ~/virtual_environments/healthchecks/bin/activate"
alias vpyright="source ~/virtual_environments/pyright/bin/activate"
alias vrune="source ~/virtual_environments/runescape/bin/activate"
alias vbots="source ~/virtual_environments/bots/bin/activate"
#10.docker
alias daws="aws ecr get-login-password --region us-west-2 | sudo docker login --username AWS --password-stdin 586672212047.dkr.ecr.us-west-2.amazonaws.com"
alias dpull="sudo docker pull 586672212047.dkr.ecr.us-west-2.amazonaws.com/secops-ansible:173"
alias drun="docker run -it 586672212047.dkr.ecr.us-west-2.amazonaws.com/secops-ansible:173 /bin/bash"
#11.wireguard
alias wgdown="sudo wg-quick down wg1"
alias wgup="sudo wg-quick up wg1"
alias wgshow="sudo wg show wg1"
alias wgrestart="sudo wg-quick down wg1 && sudo wg-quick up wg1"
